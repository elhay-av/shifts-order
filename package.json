{
  "name": "shiftsOrder",
  "version": "0.0.0",
  "productName": "Shifts-order",
  "description": "Shifts order app",
  "author": {
    "name": "Elhay Avihezer",
    "email": "elhayav@gmail.com",
    "url": "https://github.com/elhay-av"
  },
  "dependencies": {
    "electron-squirrel-startup": "^0.1.4",
    "electron-json-storage": "^2.0.0"
  },
  "main": "dist/main.js",
  "build": {
    "app-bundle-id": "elhay.shift.app",
    "app-category-type": "public.app-category.productive",
    "iconUrl": "https://raw.githubusercontent.com/elhay-av/shifts-order/master/src/favicon.ico",
    "asar": false,
    "productName": "Shifts-order",
    "osx": {
      "app-bundle-id": "elhay.shift.app",
      "app-category-type": "public.app-category.productive",
      "title": "Shifts order app",
      "icon-size": 128,
      "contents": [
        {
          "x": 355,
          "y": 125,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 155,
          "y": 125,
          "type": "file"
        }
      ]
    },
    "win": {
      "title": "Shifts order app",
      "icon": "assets/images/favico.ico"
    }
  },
  "directories": {
    "output": "./installer"
  },
  "scripts": {
    "start": "electron dist/main.js",
    "sign": "export CSC_NAME=\"co.il.elhay.shifts-order:elhay\"",
    "test": "gulp test",
    "clean": "rm -rf ./installer && rm -rf ./app",
    "compile": "gulp && cp -r dist/. app/ && cp app-package.json app/package.json",
    "pre-build": "npm run clean && npm run compile && npm run sign",
    "postinstall": "install-app-deps",
    "pack": "npm run pre-build && build",
    "dist": "npm run pre-build && build",
    "dist:win32": "npm run pre-build && build --platform win32 --arch ia32",
    "dist:win64": "npm run pre-build && build --platform win32 --arch x64"
  },
  "devDependencies": {
    "babel-core": "~6.7.4",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.6.0",
    "browser-sync": "~2.9.11",
    "browser-sync-spa": "~1.0.3",
    "chalk": "~1.1.1",
    "del": "~2.0.2",
    "electron-builder": "^3.6.1",
    "electron-packager": "^6.0.2",
    "electron-prebuilt": "^0.36.0",
    "eslint-loader": "~1.1.0",
    "eslint-plugin-angular": "~0.12.0",
    "estraverse": "~4.1.0",
    "gulp": "~3.9.0",
    "gulp-angular-templatecache": "~1.8.0",
    "gulp-autoprefixer": "~3.0.2",
    "gulp-cssnano": "~2.1.1",
    "gulp-eslint": "~1.0.0",
    "gulp-filter": "~3.0.1",
    "gulp-flatten": "~0.2.0",
    "gulp-htmlmin": "~1.3.0",
    "gulp-inject": "~3.0.0",
    "gulp-load-plugins": "~0.10.0",
    "gulp-protractor": "~2.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "~0.5.4",
    "gulp-rev": "~6.0.1",
    "gulp-rev-replace": "~0.4.2",
    "gulp-sass": "~2.0.4",
    "gulp-size": "~2.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.4.1",
    "gulp-useref": "~3.0.3",
    "gulp-util": "~3.0.6",
    "http-proxy-middleware": "~0.9.0",
    "karma": "~0.13.10",
    "karma-coverage": "~0.5.2",
    "karma-jasmine": "~0.3.6",
    "karma-ng-html2js-preprocessor": "~0.2.0",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-phantomjs-shim": "~1.2.0",
    "lodash": "~3.10.1",
    "main-bower-files": "~2.9.0",
    "ng-annotate-loader": "0.0.10",
    "phantomjs": "~1.9.18",
    "uglify-save-license": "~0.4.1",
    "webpack-stream": "~2.1.1",
    "wiredep": "~2.2.2",
    "wrench": "~1.5.8"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  }
}
